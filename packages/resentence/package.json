{
  "name": "resentence",
  "version": "0.3.1",
  "description": "Easy-to-use React component for morphing one string into another.",
  "main": "dist/index.js",
  "types": "dist/index",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dphilipson/resentence.git"
  },
  "keywords": [
    "react",
    "Levenshtein",
    "text",
    "animation"
  ],
  "homepage": "https://github.com/dphilipson/resentence",
  "bugs": {
    "url": "https://github.com/dphilipson/resentence/issues",
    "email": "david.philipson@gmail.com"
  },
  "author": "David Philipson <david.philipson@gmail.com> (http://dphil.me)",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean && tsc",
    "clean": "rm -rf dist/*",
    "lint-file": "tslint",
    "lint": "tslint --project .",
    "pre-commit": "lint-staged",
    "prepublishOnly": "yarn run test && yarn run build",
    "start": "tsc --watch",
    "test": "yarn run lint && yarn run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn run lint-file --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "lint-staged": "^8.1.5",
    "tslint": "^5.14.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react-spring": "^8.0.18"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  }
}
